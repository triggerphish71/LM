<cfquery name = "ChargeInsert" datasource = "#application.datasource#">
	insert TenantPromotionSet 
		 (cDescription
           ,dtEffectiveStart
           ,dtEffectiveEnd
           ,iRowStartUser_ID
           ,dtRowStart
           )
		values
		( '#form.cDescription#',
		   '#form.dtEffectiveStart#',
		   '#form.dtEffectiveEnd#',
		   #session.UserID#,
		   getDate()
				)
				
</cfquery>
<cflocation url="TenantPromotions.cfm" ADDTOKEN="No">
