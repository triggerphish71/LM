<cfoutput>

	<cfquery name="BondAptUpdate" datasource="#application.datasource#">
			update AptAddress
			set bIsBond = '#form.iBondApt#'
			, dtRowStart  = GetDate()
			, iRowStartUser_ID = #SESSION.USERID#
			where iAptAddress_ID= '#form.iAptAddress_ID#'
	</cfquery>
	
<cflocation url="BondInfo.cfm" ADDTOKEN="No">
</cfoutput>


 
