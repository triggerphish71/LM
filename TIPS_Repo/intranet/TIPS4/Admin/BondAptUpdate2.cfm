<cfoutput>

	<cfquery name="TurnApt2NotBond" datasource="#application.datasource#">
		update AptAddress
		Set bIsBond = 0
		where iAptAddress_ID = '#form.iAptAddress_ID2#'
	</cfquery>

	<cflocation url="BondInfo.cfm" ADDTOKEN="No">
</cfoutput>