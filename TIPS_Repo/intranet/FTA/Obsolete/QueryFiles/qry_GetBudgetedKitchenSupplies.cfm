<cfparam name="yearforqueries" default="#DatePart("y",now())#">
<cfparam name="#houseId# " default="0">

<cfquery name="GetBudgetedKitchenSupplies" datasource="#ComshareDS#">
	select 
		*
	from 
		ALC.FINLOC_BASE
	where
		year_id= #yearforqueries#   
	and
		Line_id= 80000616  
	and 
		unit_id= #houseId# 
	and
		ver_id=  1  
	and
		Cust1_id= 0  
	and
		Cust2_id= 0  
	and
		Cust3_id= 0  
	and
		Cust4_id= 0  
</cfquery>